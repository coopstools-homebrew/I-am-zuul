{
    "name": "coopstools-ghsso",
    "description": "Service that integrates github with the coopstools platform",
    "keywords": ["coopstools", "coopstools-homebrew", "github", "sso", "auth"],
    "repository": "https://github.com/coopstools-homebrew/I-am-zuul",
    "buildpacks": [
      {
        "url": "heroku/go"
      }
    ],
    "env": {
        "GITHUB_CLIENT_ID": {
            "required": true
        },
        "GITHUB_CLIENT_SECRET": {
            "required": true
        },
        "GITHUB_REDIRECT_URI": {
            "required": true
        },
        "PRIVATE_KEY": {
            "required": true
        },
        "PUBLIC_KEY": {
            "required": true
        }
    }
}
